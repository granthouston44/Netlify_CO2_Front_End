,CAAA,EAAA,MAAA,CAAA,WAAA,GAAA,GAAA;AACA,MAAA,WAAA,CAAA,IAAA,CAAA,KAAA,UAAA,CAAA,GAAA,GAAA,KAAA,aAAA,CAAA,CAAA,EAAA,MAAA,CAAA,GAAA,GAAA,GAAA;AACA,MAAA,WAAA,CAAA,IAAA,CAAA,KAAA,UAAA,CAAA,MAAA,GAAA,KAAA,aAAA,CAAA,CAAA,EAAA,MAAA,CAAA,MAAA,GAAA,GAAA;;AAEA,UAAA,SAAA,GAAA,SAAA,SAAA,CAAA,SAAA,EAAA;AACA,eAAA,SAAA,CAAA,MAAA,CAAA,UAAA,KAAA,EAAA,MAAA,EAAA;AACA,iBAAA,KAAA,GAAA,MAAA;AACA,SAFA,EAEA,CAFA,CAAA;AAGA,OAJA;;AAKA,MAAA,QAAA,CAAA,KAAA,CAAA,mBAAA,EAAA,SAAA,CAAA,WAAA,CAAA;AACA,aAAA,SAAA,CAAA,WAAA,CAAA,CAAA,WAAA,CAAA,CAAA,CAAA;AACA;AAdA;AAFA,CAAA","sourcesContent":["<template lang=\"html\">\n  <div v-if=\"emissionsProp && energyProp\">\n    <h4>Energy Total: {{energyTotal}}kg CO2e/day</h4>\n  </div>\n  <div v-else>\n    <h4>Energy Total: </h4>\n  </div>\n</template>\n\n<script>\nimport {eventBus} from \"@/main.js\"\n\nexport default {\n  props: ['emissionsProp', 'energyProp'],\n  computed:{\n    energyTotal(){\n      let energyArray = []\n      energyArray.push((this.energyProp.electricity * this.emissionsProp[0].energy.electricity) / 365)\n      energyArray.push((this.energyProp.gas * this.emissionsProp[0].energy.gas) / 365)\n      energyArray.push((this.energyProp.hybrid * this.emissionsProp[0].energy.hybrid)/ 365)\n\n    const sumEnergy = function(someArray){\n      return someArray.reduce((total, energy) => {\n          return total + energy\n      }, 0)\n    }\n  eventBus.$emit('energy-calculated', sumEnergy(energyArray))\n  return sumEnergy(energyArray).toPrecision(2)\n    }\n  }\n}\n</script>\n\n<style lang=\"css\" scoped>\n</style>\n"],"sourceRoot":"src/components"}]}